    "css-loader": "^3.4.1",
    "cz-conventional-changelog": "3.0.2",
    "gatsby-plugin-mdx": "^1.0.17",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-remark-prismjs": "^3.3.30",
    "mdx-utils": "^0.2.0",
    "mini-css-extract-plugin": "^0.9.0",
