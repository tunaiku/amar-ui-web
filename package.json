  "license": "MIT",
  "author": "dkk94 <davidkurniakristiadi@gmail.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*",
    "library/*"
  ],
  "scripts": {
    "build": "yarn build:dev && yarn build:prod",
